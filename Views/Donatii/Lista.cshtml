@model List<eUseControl.Domain.Entities.User.Donatii>


<h2>Toate Donațiile</h2>
<table class="table">
     <tr>
          <th>Nume</th>
          <th>Prenume</th>
          <th>Email</th>
          <th>Suma</th>
          <th>Data</th>
     </tr>
     @foreach (var d in Model)
     {
          <tr>
               <td>@d.Nume</td>
               <td>@d.Prenume</td>
               <td>@d.Email</td>
               <td>@d.Suma</td>
               <td>@d.DataDonatiei.ToShortDateString()</td>
          </tr>
     }
</table>
